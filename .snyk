version: v1.29.0

# Ignore rules (optional)
ignore:
  # Example: Ignore a known safe `unwrap()` usage
  error:unwrap-used:
    - '*':
        reason: "Reviewed; safe unwrap."
        expires: "2026-01-01T00:00:00Z"

# Code settings for Snyk Code
code:
  enabled: true
  languages:
    - rust
  exclude:
    - target/
    - tests/
    - .git/
